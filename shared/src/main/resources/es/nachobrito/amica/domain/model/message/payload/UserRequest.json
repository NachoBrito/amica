{
  "$id": "UserRequest",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "UserRequest",
  "type": "object",
  "required": [
    "dateTime",
    "message"
  ],
  "properties": {
    "dateTime": {
      "type": "string",
      "format": "date-time"
    },
    "userName": {
      "type": "string",
      "description": "The person's user name (optional)"
    },
    "message": {
      "type": "string",
      "description": "The person's message."
    }
  }
}

